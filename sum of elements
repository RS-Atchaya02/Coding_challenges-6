function sumEvenIndices(arr) {
    return arr.reduce((sum, currentValue, index) => {
        if (index % 2 === 0) {
            return sum + currentValue;
        }
        return sum;
    }, 0);
}
const numbers = [1, 3, 5, 7, 9];
const sum = sumEvenIndices(numbers);
console.log(sum);  // Output will be 15 (1 + 5 + 9)
